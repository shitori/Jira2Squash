{
    "root": true,
    "plugins": ["jest"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "prettier"],
    "env": {
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "rules": {
        "no-console": ["log", { "allow": ["info", "warn", "error"] }]
    }
}
